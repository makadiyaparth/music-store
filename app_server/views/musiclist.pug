extends layout

block content
  h2= title
  h3 Here is a list of popular musics across the globe...
  table.table.table-striped.table-light
    thead
      td.fw-bold Rank
      td.fw-bold Album
      td.fw-bold Artist
      td.fw-bold Songs 
      td.fw-bold Year Of Release 
    tbody
      each music in musics 
        tr  
          td #{music.rank}
          td #{music.album}
          td #{music.artist}
          td #{music.songs}
          td #{music.yearOfRelease}

  p 
    a(href=`/new`) Create new Album

            
